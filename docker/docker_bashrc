export PATH=/root/labbo/toolchain/arm-gnu-toolchain-11.3.rel1-x86_64-aarch64-none-linux-gnu/bin:$PATH
export PATH=/root/labbo/toolchain/arm-gnu-toolchain-11.3.rel1-x86_64-arm-none-linux-gnueabihf/bin:$PATH
export PATH=/root/labbo/docker/bin:$PATH

cat << EOF
LABBO SYSTEM CHEAT SHEET:
	1. qrunarm /path/to/Image [gdb]: launch qemu arm
	2. qrunarm64 /path/to/Image [gdb]: launch qemu arm64
	3. telnet localhost 54320: connect qemu linux console
	4. ssp -p 8765 ubuntu@localhost: connect qemu vm via ssh
EOF
