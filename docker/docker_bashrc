export PATH=/root/labbo/toolchain/arm-gnu-toolchain-11.3.rel1-x86_64-aarch64-none-linux-gnu/bin:$PATH
export PATH=/root/labbo/docker/bin:$PATH

cat << EOF
run 'qrun /path/to/share_dir' to launch qemu aarch64 vm.

run 'telnet localhost 54320' to connect to vm console
run 'ssh -p 8765 ubuntu@localhost' to connect vm via ssh

EOF
